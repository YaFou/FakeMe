{
    "name": "yafou/fakeme",
    "description": "A ridiculous, simple and powerful fake data generator",
    "keywords": [
        "faker",
        "data",
        "generator"
    ],
    "prefer-stable": true,
    "type": "library",
    "minimum-stability": "stable",
    "homepage": "https://github.com/YaFou/FakeMe",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannis Foucher",
            "homepage": "https://github.com/YaFou"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3",
        "ramsey/uuid": "^4.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "YaFou\\FakeMe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YaFou\\FakeMe\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
